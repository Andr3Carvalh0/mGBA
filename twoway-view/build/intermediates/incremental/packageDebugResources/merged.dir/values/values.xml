<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <declare-styleable name="twowayview_DividerItemDecoration">
        <attr name="android:divider" ns1:ignore="ResourceName"/>
        <attr format="reference" name="twowayview_verticalDivider"/>
        <attr format="reference" name="twowayview_horizontalDivider"/>
    </declare-styleable>
    <declare-styleable name="twowayview_GridLayoutManager">
        <attr format="integer" name="twowayview_numColumns"/>
        <attr format="integer" name="twowayview_numRows"/>
    </declare-styleable>
    <declare-styleable name="twowayview_SpacingItemDecoration">
        <attr name="android:verticalSpacing" ns1:ignore="ResourceName"/>
        <attr name="android:horizontalSpacing" ns1:ignore="ResourceName"/>
    </declare-styleable>
    <declare-styleable name="twowayview_SpannableGridViewChild">
        <attr format="integer" name="twowayview_colSpan"/>
        <attr format="integer" name="twowayview_rowSpan"/>
    </declare-styleable>
    <declare-styleable name="twowayview_StaggeredGridViewChild">
        <attr format="integer" name="twowayview_span"/>
    </declare-styleable>
    <declare-styleable name="twowayview_TwoWayLayoutManager">
        <attr name="android:orientation" ns1:ignore="ResourceName"/>
    </declare-styleable>
    <declare-styleable name="twowayview_TwoWayView">
        <attr format="string" name="twowayview_layoutManager"/>
        <attr format="boolean" name="twowayview_hideScrollbar"/>
        <attr format="boolean" name="twowayview_showBubble"/>
        <attr format="boolean" name="twowayview_showTrack"/>
        <attr format="color" name="twowayview_trackColor"/>
        <attr format="color" name="twowayview_handleColor"/>
        <attr format="color" name="twowayview_bubbleColor"/>
        <attr format="color" name="twowayview_bubbleTextColor"/>
    </declare-styleable>
    <dimen name="twowayview_bubble_padding">16dp</dimen>
    <dimen name="twowayview_bubble_radius">44dp</dimen>
    <dimen name="twowayview_bubble_size">88dp</dimen>
    <dimen name="twowayview_bubble_textsize">48sp</dimen>
    <dimen name="twowayview_handle_height">40dp</dimen>
    <dimen name="twowayview_handle_radius">2dp</dimen>
    <dimen name="twowayview_handle_width">4dp</dimen>
    <dimen name="twowayview_scrollbar_margin_bottom">8dp</dimen>
    <dimen name="twowayview_scrollbar_margin_top">8dp</dimen>
    <dimen name="twowayview_scrollbar_padding_end">8dp</dimen>
    <dimen name="twowayview_scrollbar_padding_start">8dp</dimen>
    <dimen name="twowayview_track_width">1dp</dimen>
    <item name="twowayview_fastscroller" type="id"/>
    <item name="twowayview_item_click_support" type="id"/>
    <item name="twowayview_item_selection_support" type="id"/>
</resources>